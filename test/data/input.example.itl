   -00:01:00 -00:02:00 +00:02:00 \
                 OSIRIS    INCLUDE       "image_sequence.itl"
000_13:13:00     MIDAS     STANDBY       MOVE_ARM (
                                            ROLL = 12.0 [degrees] \
                                            YAW = 33.0 [degrees] \
                                            ARM_MODE = "MODE B" \
                                            POWER_PROFILE = \
                                               00:00:00   1.234 [Watts] \
                                               00:01:00   2.345 [Watts] \
                                               00:02:00   3.456 [Watts] \
                                            DATA_RATE_PROFILE = \
