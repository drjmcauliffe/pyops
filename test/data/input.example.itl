                           "... with some optional text" \
                           "and some more text"
000_12:45:20     MIDAS     MEASUREMENT   MAKE_IMAGE ( \
                                            IMAGE_TIME = 60.0 [seconds] \
                                            REPEAT = 3 \
                                            SEPARATION = 00:02:00)
ALT_BELOW_500M (COUNT = 712 RESOLVE = ORIGINAL) \
                 OSIRIS    MEASUREMENT   START_TESTS
000_13:02:45     MIDAS     MEASUREMENT   MEASUREMENT_TO_STANDBY
000_13:08:00     MIDAS     STANDBY       MOVE_ARM ( \
                                            ROLL = 15.2 [degrees] \
                                            YAW = 8.8 [degrees] \
                                            ARM_MODE = "MODE A")
