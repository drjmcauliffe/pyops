000_12:05:00     OSIRIS    STANDBY       STANBY_TO_MEASUREMENT
000_12:10:00     MIDAS     MEASUREMENT   MAKE_IMAGE (
                                            IMAGE_TIME = 30.0 [seconds])
000_12:15:00     INCLUDE   "generic_sequence.itl"
000_12:35:50 -01:00:00 +01:30:00 \
                 MIDAS     MEASUREMENT   MAKE_IMAGE ( \
                                            IMAGE_TIME = 60.0 [seconds])
